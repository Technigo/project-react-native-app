{
	"expo": {
		"name": "project-react-native-app",
		"slug": "An essential quote-generating app",
		"privacy": "public"
		"description": "Need a qoute? Here's your app! Choose between the sayings of great thinkers and the shoughts of Kanye West. An app bulit with React Native by Technigo student Maria Petersson."
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
