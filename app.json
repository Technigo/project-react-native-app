{
  "expo": {
    "name": "Jakobs Native App",
    "description": "A app with a shake function and geo location function",
    "githubUrl": "https://github.com/jakobxlindstrom/project-react-native-app",
    "slug": "Jakobs-Native-App",
    "version": "1.0.0",
    "sdkVersion": "43.0.0",
    "backgroundColor": "#00467F",
    "primaryColor": "#A5CC82",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
