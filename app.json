{
  "expo": {
    "name": "Sunset Driving",
	"description": "A car-driving app using accelerometer sensor",
	"githubUrl": "https://github.com/loulunds/project-react-native-app",
	"orientation": "portrait",
    "slug": "Sunset-Driving",
    "version": "1.0.0",
	"sdkVersion": "43.0.0",
	"backgroundColor": "#7f2cf82",
	"primaryColor" : "#7f2cf8",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
